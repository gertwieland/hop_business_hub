      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var Status = null;
var Validity = null;

//Status
switch (true) {
	case STATUSID == 0:
		Status = 'Inactive';
		break;

	case STATUSID == 1:
		Status = 'Active';
		break;

	case STATUSID == 2:
		Status = 'Deleted';
		break;

	default:
		Status = '***Unknown***';

}



//Validity

switch (true) {
	case VALIDID == 0:
		Validity = 'Unmodified';
		break;


	case VALIDID == 1:
		Validity = 'Unverified - Deleted';
		break;

	case VALIDID == 2:
		Validity = 'Unverified';
		break;

	case VALIDID == 3:
		Validity = 'Rejected';
		break;

	case VALIDID == 4:
		Validity = 'Deleted';
		break;

	case VALIDID == 5:
		Validity = 'Depricated';
		break;

	case VALIDID == 10:
		Validity = 'Verified';
		break;

	case VALIDID == -1:
		Validity = 'Temporary';
		break;

	default:
		Validity = '***Validity Unknown***';
}</jsScript_script>
