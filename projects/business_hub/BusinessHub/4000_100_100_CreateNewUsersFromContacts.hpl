EmailEncodingKey = 'UTF-8';

//Regex removes blanks
FirstNameWithoutSpaces = lower(FirstName.replace(/ /g,''))
LastNameWithoutSpaces = lower(LastName.replace(/ /g,''))

//Set Alias
switch (true) {
	case FirstName.length >= 0 &amp;&amp; LastName.length >= 0:
		Alias = substr(FirstNameWithoutSpaces,0,1).concat(substr(LastNameWithoutSpaces,0,7))
		break;
}

Email = lower(Email)
UserName = Email;

//Set NickName
switch (true) {
	case FirstName !== null &amp;&amp; LastName !== null:
		CommunityNickName = substr(FirstName.concat(" ").concat(LastName),0,40);
		break;
}


//If user is null but was found via email, then use that ID
switch (true) {
	case User === null &amp;&amp; UserID_FoundViaEmail !== null:
		User = UserID_FoundViaEmail;
		break;
}


//Assign User Profiles
switch (true) {

	case IsPI == 'Yes':
		//Assign Profile "Account Holder User" to PIs
		ProfileID = '00e5W000000LsIa';

		//Enable Portal
		IsPortalEnabled = true;

		//If already active, leave active, otherwise set to inactive (relevant only if null)
		if (IsPortalActive != 'true') {
			IsPortalActive = 'false'
			}

		break;



	//Use for Non-Account-Holders (need to find different rule for Institute Managers)
	case IsPI !== 'Yes':
		//Assign Profile "Account Holder User" to PIs
		ProfileID = '00e5W000000LsU2';

		//Enable Portal
		IsPortalEnabled = true;

		//If already active, leave active, otherwise set to inactive (relevant only if null)
		if (IsPortalActive != 'true') {
			IsPortalActive = 'false'
			}

		break;

}

//Convert IsPortalActive to boolean
if (IsPortalActive === 'true') {
	IsPortalActive = true
} else {
	IsPortalActive = false
}

	

//Assign Roles
